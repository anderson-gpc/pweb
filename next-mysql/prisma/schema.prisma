generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./movies.db"
}

model Movie {
  id           String   @id @default(cuid())
  title        String
  year         String
  poster       String?
  imdbID       String
  type         MediaType
}

enum MediaType {
  movie
  series
}
